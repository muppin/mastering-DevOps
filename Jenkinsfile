pipeline{
    agent any
    stages{
        stage("build"){
            steps{
                sh 'echo building the application'
            }
        }
        stage("test"){
            steps{
                sh 'echo testing the application'
                
            }
        }
        stage("Run"){
            steps{
                sh 'echo running the application'
                
            }
        }
        stage("deploy"){
            steps{
                sh 'echo deploying the application'
                
            }
        }
    }
}
